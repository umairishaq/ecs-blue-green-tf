{
    "family": "GreenTaskDefinition",
    "taskRoleArn": "TASK ROLE",
    "executionRoleArn": "TASK EXEC ROLE",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "awesome-api",
            "image": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "MyTestStack2-ServiceLogGroup-15RBFZI8YJIA8",  
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "Service"
                },
                "secretOptions": []
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "inferenceAccelerators": []
}